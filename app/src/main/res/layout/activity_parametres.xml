<?xml version="1.0" encoding="utf-8"?>
<ca.cours5b5.charleslangevin.vues.VParametres xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.AParametres"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/lblHeight"
        android:text="@string/pHeight"

        android:layout_height="wrap_content"
        android:layout_width="wrap_content"

        android:layout_marginLeft="35dp"
        android:layout_marginTop="100dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/spinHeight"
        app:layout_constraintBottom_toTopOf="@+id/lblWidth"/>

    <TextView
        android:id="@+id/lblWidth"
        android:text="@string/pWidth"

        android:layout_height="wrap_content"
        android:layout_width="wrap_content"

        android:layout_marginLeft="0dp"
        android:layout_marginTop="25dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"

        app:layout_constraintBottom_toTopOf="@+id/lblWin"
        app:layout_constraintLeft_toLeftOf="@id/lblHeight"
        app:layout_constraintRight_toLeftOf="@+id/spinWidth"
        app:layout_constraintTop_toBottomOf="@id/lblHeight" />

    <TextView
        android:id="@+id/lblWin"
        android:text="@string/pWin"

        android:layout_height="wrap_content"
        android:layout_width="wrap_content"

        android:layout_marginLeft="0dp"
        android:layout_marginTop="25dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"

        app:layout_constraintLeft_toLeftOf="@id/lblWidth"
        app:layout_constraintTop_toBottomOf="@id/lblWidth"
        app:layout_constraintRight_toLeftOf="@+id/spinWin"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <Spinner
        android:id="@+id/spinHeight"

        android:layout_width="100dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="25dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"

        app:layout_constraintLeft_toRightOf="@id/lblHeight"
        app:layout_constraintTop_toTopOf="@id/lblHeight"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@id/spinWidth"/>

    <Spinner
        android:id="@+id/spinWidth"

        android:layout_width="100dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="25dp"
        android:layout_marginTop="25dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"

        app:layout_constraintLeft_toRightOf="@id/lblWidth"
        app:layout_constraintTop_toBottomOf="@id/spinHeight"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@id/spinWin"/>

    <Spinner
        android:id="@+id/spinWin"

        android:layout_width="100dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="25dp"
        android:layout_marginTop="25dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"

        app:layout_constraintLeft_toRightOf="@id/lblWin"
        app:layout_constraintTop_toBottomOf="@id/spinWidth"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <android.support.constraint.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <android.support.constraint.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</ca.cours5b5.charleslangevin.vues.VParametres>