<?xml version="1.0" encoding="utf-8"?>
<ca.cours5b5.charleslangevin.vues.VParametres
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.AParametres"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/lblHeight"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"

        android:layout_marginStart="35dp"
        android:layout_marginTop="50dp"
        android:text="@string/pHeight"
        app:layout_constraintBottom_toTopOf="@+id/lblWidth"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblWidth"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"

        android:layout_marginTop="20dp"
        android:text="@string/pWidth"
        app:layout_constraintBottom_toTopOf="@+id/lblWin"
        app:layout_constraintStart_toStartOf="@+id/lblHeight"
        app:layout_constraintTop_toBottomOf="@+id/lblHeight"
        app:layout_constraintVertical_chainStyle="spread_inside" />

    <TextView
        android:id="@+id/lblWin"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginBottom="50dp"

        android:layout_marginLeft="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="20dp"
        android:text="@string/pWin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/lblWidth"
        app:layout_constraintTop_toBottomOf="@+id/lblWidth" />

    <Spinner
        android:id="@+id/spinHeight"

        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginBottom="0dp"
        android:layout_marginEnd="20dp"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintBottom_toTopOf="@+id/spinWidth"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/lblHeight"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinWidth"

        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginBottom="0dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        app:layout_constraintBottom_toTopOf="@+id/spinWin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/spinHeight"
        app:layout_constraintTop_toBottomOf="@+id/spinHeight"
        tools:layout_editor_absoluteY="295dp" />

    <Spinner
        android:id="@+id/spinWin"

        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginBottom="50dp"
        android:layout_marginEnd="20dp"
        android:layout_marginLeft="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/spinWidth"
        app:layout_constraintTop_toBottomOf="@+id/spinWidth" />

    <android.support.constraint.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <android.support.constraint.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</ca.cours5b5.charleslangevin.vues.VParametres>